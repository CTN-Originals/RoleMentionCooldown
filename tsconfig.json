{
	"compilerOptions": {
		"declaration": false,
		"module": "CommonJS",
		"moduleResolution": "Node",
		"noImplicitAny": false,
		"outDir": "dist",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"strictNullChecks": true,
		"target": "ES2022",
		"typeRoots": [
			"src/@types",
			"node_modules/@types"
		]
	},
	"exclude": [
		"node_modules"
	],
	"files": [
		"src/@types/discord.d.ts"
	],
	"include": [
		"src"
	]
}
